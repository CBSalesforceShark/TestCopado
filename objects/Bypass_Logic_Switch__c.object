    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Bypass_Trigger__c</fullName>
        <defaultValue>false</defaultValue>
        <description>This field is utilized to bypass the trigger logic</description>
        <externalId>false</externalId>
        <label>Bypass Trigger</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Bypass_Validation_Rule__c</fullName>
        <defaultValue>false</defaultValue>
        <description>This field is utilized to bypass the validation rule</description>
        <externalId>false</externalId>
        <label>Bypass Validation Rule</label>
        <trackTrending>false</trackTrending>
